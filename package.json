{
  "name": "genshin-music",
  "engines": {
    "node": "16.13.0"
  },
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@inlet/react-pixi": "^6.6.5",
    "@pixi/graphics-smooth": "^0.0.22",
    "@tauri-apps/api": "^1.0.2",
    "@tonejs/midi": "^2.0.27",
    "audio-recorder-polyfill": "^0.4.1",
    "audiobuffer-to-wav": "^1.0.0",
    "cli-color": "^2.0.2",
    "color": "^4.2.0",
    "dotenv": "^16.0.1",
    "is-mobile": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "mobx": "^6.3.7",
    "pepjs": "^0.5.3",
    "pixi.js": "^6.1.3",
    "react": "^17.0.2",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "recursive-copy": "^2.0.13",
    "typescript": "^4.5.5",
    "use-font-face-observer": "^1.2.1",
    "web-vitals": "^2.1.4",
    "workbox-core": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "worker-timers": "^7.0.45",
    "zangodb": "^1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "tauri": "tauri",
    "build-tauri-deployment": "cross-env REACT_APP_IS_TAURI=true node ./scripts/buildAndPrepareTauriRelease.js true",
    "build-tauri-deployment:no-env": "cross-env REACT_APP_IS_TAURI=true node ./scripts/buildAndPrepareTauriRelease.js false",
    "build-tauri:sky": "cross-env REACT_APP_IS_TAURI=true env-cmd yarn tauri build --config ./src-tauri/tauri-sky.conf.json",
    "build-tauri:sky-no-env": "cross-env REACT_APP_IS_TAURI=true yarn tauri build --config ./src-tauri/tauri-sky.conf.json",
    "build-tauri:genshin": "cross-env REACT_APP_IS_TAURI=true env-cmd yarn tauri build --config ./src-tauri/tauri-genshin.conf.json",
    "build-tauri:genshin-no-env": "cross-env REACT_APP_IS_TAURI=true yarn tauri build --config ./src-tauri/tauri-genshin.conf.json",
    "build-tauri:all": "cross-env REACT_APP_IS_TAURI=true yarn build-tauri:sky && yarn build-tauri:genshin",
    "build:sky": "node ./scripts/buildApp.js Sky",
    "build:genshin": "node ./scripts/buildApp.js Genshin",
    "build:all": "node ./scripts/buildApp.js All",
    "start-tauri:sky": "yarn tauri dev --config ./src-tauri/tauri-sky.conf.json",
    "start-tauri:genshin": "yarn tauri dev --config ./src-tauri/tauri-genshin.conf.json",
    "start:sky": "node ./scripts/startApp.js Sky",
    "start:genshin": "node ./scripts/startApp.js Genshin"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.1",
    "@types/color": "^3.0.3",
    "@types/jest": "^27.4.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webmidi": "^2.0.6",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0"
  }
}
